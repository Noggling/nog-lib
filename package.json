{
  "name": "nog-lib",
  "version": "0.0.1",
  "description": "nog-lib initial typescript library",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "tsc && node ./dist/index.js",
    "docs": "npx typedoc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx src --color --fix",
    "lint-strict": "eslint --ext .js,.jsx,.ts,.tsx src --color --max-warnings=0",
    "test": "jest",
    "test-watch": "npm ci & jest --watchAll",
    "test-coverage": "npm ci & jest --coverage"
  },
  "keywords": [
    "lib",
    "nog",
    "typescript"
  ],
  "author": "Nogglings",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "jest-expo": "^41.0.0",
    "prettier": "^2.3.1",
    "typedoc": "^0.20.37",
    "typescript": "^4.3.4"
  }
}
